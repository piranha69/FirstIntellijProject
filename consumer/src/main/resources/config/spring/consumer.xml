<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-init-method="initService" default-destroy-method="destroyService">

    <bean id="messageConsumer" class="cz.tomanjir.messaging.TestMessageConsumer"/>

    <bean id="testReceiver" class="cz.tomanjir.messaging.rabbitmq.RabbitMqMessageReceiver">
        <constructor-arg index="0" ref="connector"/>
        <constructor-arg index="1" value="hs-test"/>
        <constructor-arg index="2" ref="messageConsumer"/>
    </bean>
</beans>